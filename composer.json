{
    "name": "minemindmedia/laravel-mmmedia",
    "description": "A Laravel package providing a global media library and Filament MediaPicker field",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "filament", "media", "library", "upload", "images", "videos", "documents"],
    "homepage": "https://github.com/minemindmedia/laravel-mmmedia",
    "support": {
        "issues": "https://github.com/minemindmedia/laravel-mmmedia/issues",
        "source": "https://github.com/minemindmedia/laravel-mmmedia"
    },
    "authors": [
        {
            "name": "MineMind Media",
            "email": "info@minemindmedia.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "filament/filament": "^3.0",
        "spatie/laravel-medialibrary": "^10.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Mmmedia\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmmedia\\Media\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mmmedia\\Media\\MediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
